{
    "animations": {
      "lipMovement": {
        "frames": [
          { "time": 0, "mouthShape": "neutral" },
          { "time": 0.2, "mouthShape": "open" },
          { "time": 0.4, "mouthShape": "smile" },
          { "time": 0.6, "mouthShape": "neutral" }
        ],
        "loop": true
      },
      "eyeBlinking": {
        "frames": [
          { "time": 0, "eyeState": "open" },
          { "time": 1, "eyeState": "half-closed" },
          { "time": 1.1, "eyeState": "closed" },
          { "time": 1.2, "eyeState": "half-closed" },
          { "time": 1.3, "eyeState": "open" }
        ],
        "loop": true,
        "interval": 4
      },
      "facialExpressions": {
        "frames": [
          { "time": 0, "expression": "neutral" },
          { "time": 2, "expression": "smile" },
          { "time": 4, "expression": "surprised" },
          { "time": 6, "expression": "neutral" }
        ],
        "loop": true
      }
    }
  }
  